{
  "train_dataset": {
    "root_path": "../../dataset/trainsets/color",
    "kernel_path": "../../dataset/kernels/kernels_19.mat",
    "patch_size": 256,
    "is_train": false,
    "num_channels": 3,
    "num_workers": 8,
    "sigma_min": 0,
    "sigma_max": 25,
    "batch_size": 8
  },

  "val_dataset": {
    "root_path": "../../dataset/testsets/colortest",
    "kernel_path": "../../dataset/kernels/kernels_19.mat",
    "num_channels": 3,
    "noise_level": 2.55,
    "kernel_index": 8
  },

  "model": {
    "model_select": "NRL_GDN",
    "model_save_file": "pretrainedmodels",
