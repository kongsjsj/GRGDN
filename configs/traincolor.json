    "is_train_jpeg_compression": false,
    "is_train_poisson_gaussian": false,
    "is_train_impulse": false,
    "seed": 123,
    "disable_cuda": true,
    "device_ids": [0, 1],
    "log_file": "deblur_logger_NRL_GDN.txt"
